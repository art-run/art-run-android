<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".member_management.SelectSettingsActivity">
    <Button
        android:id="@+id/bt_back"
        android:layout_width="50dp"
        android:layout_height="55dp"
        android:layout_marginStart="7dp"
        android:layout_marginTop="13dp"
        android:text="←"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout" />
    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="20dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="36dp"
        android:text="개인 정보 입력"
        android:textSize="15sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view" />

    <TextView
        android:id="@+id/textNickname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="9dp"
        android:text="ExampleID"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imgBtnProfile" />

    <View
        android:id="@+id/view3"
        android:layout_width="360dp"
        android:layout_height="1dp"
        android:layout_marginStart="29dp"
        android:layout_marginTop="12dp"
        android:background="#686868"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <View
        android:id="@+id/view"
        android:layout_width="150dp"
        android:layout_height="1.5dp"
        android:layout_marginTop="8dp"
        android:background="#686868"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textNickname" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="#332C2C"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="205dp" />

    <ImageButton
        android:id="@+id/imgBtnProfile"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_marginTop="74dp"
        android:scaleType="fitCenter"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout"
        app:srcCompat="@drawable/example_picture"
        tools:ignore="SpeakableTextPresentCheck" />

    <Button
        android:id="@+id/bt_bodyInfoEdit"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:background="#D6D3D3"
        android:backgroundTint="#000000"
        android:gravity="center"
        android:text="신체 정보 입력"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.513"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view3" />

    <Button
        android:id="@+id/bt_editAccountInfo"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:background="#D6D3D3"
        android:backgroundTint="#000000"
        android:gravity="center"
        android:text="계정 정보 수정"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.513"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bt_bodyInfoEdit" />

    <Button
        android:id="@+id/bt_runningSetting"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="11dp"
        android:background="#000000"
        android:backgroundTint="#000000"
        android:gravity="center"
        android:text="러닝 설정"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.513"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bt_editAccountInfo" />

    <Button
        android:id="@+id/bt_logout"
        android:layout_width="120dp"
        android:layout_height="38dp"
        android:layout_marginTop="28dp"
        android:background="#D6D3D3"
        android:backgroundTint="#CD0808"
        android:gravity="center"
        android:text="로그아웃"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.793"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bt_runningSetting" />

    <Button
        android:id="@+id/bt_deleteAccount"
        android:layout_width="130dp"
        android:layout_height="40dp"
        android:layout_marginTop="168dp"
        android:background="#26D6D3D3"
        android:backgroundTint="#00000000"
        android:gravity="center"
        android:text="회원탈퇴"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.195"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bt_editAccountInfo" />
</androidx.constraintlayout.widget.ConstraintLayout>